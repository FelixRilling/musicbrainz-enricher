{
    "name": "musicbrainz-enricher",
    "version": "1.0.0",
    "private": true,
    "description": "Bot to enrich musicbrainz data from linked sources",
    "main": "./dist/src/main.js",
    "types": "./dist/src/main.d.ts",
    "scripts": {
        "start": "npm-run-all build serve",
        "serve": "node dist/src/main.js",
        "dist": "npm-run-all clean lint build test",
        "clean": "rimraf ./dist ./spec/dist",
        "build": "tsc --build ./tsconfig.json",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "jasmine"
    },
    "keywords": [],
    "homepage": "https://github.com/FelixRilling/musicbrainz-enricher",
    "author": {
        "name": "Felix Rilling",
        "email": "contact@rilling.dev",
        "url": "https://rilling.dev"
    },
    "repository": "https://github.com/FelixRilling/musicbrainz-enricher",
    "license": "MIT",
    "dependencies": {
        "chevronjs": "^12.0.0",
        "disconnect": "^1.2.1",
        "lightdash": "^11.1.2",
        "lodash": "^4.17.15",
        "musicbrainz-api": "^0.5.2",
        "p-retry": "^4.2.0",
        "read-pkg": "^5.2.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/jasmine": "^3.5.0",
        "@types/lodash": "^4.14.149",
        "@types/node": "^13.1.2",
        "@typescript-eslint/eslint-plugin": "^2.14.0",
        "@typescript-eslint/parser": "^2.14.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jasmine": "^3.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "typescript": "^3.7.4"
    }
}
